{
  "expo": {
    "name": "Startdii",
    "slug": "startdii",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-app.png",
    "scheme": "startdii",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ada-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
        {
          "fonts": {
            "InterLight" : ".assets/fonts/Inter_18pt-Light.ttf",
            "InterRegular" : ".assets/fonts/Inter_18pt-Regular.ttf",
            "InterMedium" : ".assets/fonts/Inter_18pt-Medium.ttf",
            "InterSemiBold" : ".assets/fonts/Inter_18pt-SemiBold.ttf",
            "InterBold" : ".assets/fonts/Inter_18pt-SemiBold.ttf",
            "IBMLight" : ".assets/fonts/IBMPlexSansThai-Light.ttf",
            "IBMRegular" : ".assets/fonts/IBMPlexSansThai-Regular.ttf",
            "IBMMedium" : ".assets/fonts/IBMPlexSansThai-Medium.ttf",
            "IBMSemiBold" : ".assets/fonts/IBMPlexSansThai-SemiBold.ttf",
            "IBMBold" : ".assets/fonts/IBMPlexSansThai-Bold.ttf"
          }
        }
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
