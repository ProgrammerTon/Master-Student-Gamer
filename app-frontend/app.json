{
  "expo": {
    "name": "Startdii",
    "slug": "startdii",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-app.png",
    "scheme": "startdii",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/wowbg2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ada-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router", "expo-font", "expo-document-picker"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
