{
  "expo": {
    "name": "Startdii",
    "slug": "startdii",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-app.png",
    "scheme": "startdii",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/wowbg2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ada-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.sompon.startdii"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-document-picker"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "59f75d0d-9e86-4942-8f4c-7b6ba1507306"
      }
    }
  }
}
